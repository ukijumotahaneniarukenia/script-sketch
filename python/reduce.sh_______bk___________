#!/bin/bash

echo {6..10} | tr ' '  ',' | awk '{cnt=gsub(",",",",$0)+1;for(i=1;i<=cnt;i++)print $0}' | \
  awk '{split($0,array,",");
    print "",
      NR,
      array[NR],
      "||",
      array[NR]+array[NR-1],
      array[NR]+array[NR-1]+array[NR-2],
      array[NR]+array[NR-1]+array[NR-2]+array[NR-3],
      array[NR]+array[NR-1]+array[NR-2]+array[NR-3]+array[NR-4],
      array[NR]+array[NR-1]+array[NR-2]+array[NR-3]+array[NR-4]+array[NR-5],
      "=>",
      array[NR]"+"array[NR-1],
      array[NR]"+"array[NR-1]"+"array[NR-2],
      array[NR]"+"array[NR-1]"+"array[NR-2]"+"array[NR-3],
      array[NR]"+"array[NR-1]"+"array[NR-2]"+"array[NR-3]"+"array[NR-4],
      array[NR]"+"array[NR-1]"+"array[NR-2]"+"array[NR-3]"+"array[NR-4]"+"array[NR-5],
      #"||",
      #array[NR-5],
      #array[NR-4]+array[NR-5],
      #array[NR-3]+array[NR-4]+array[NR-5],
      #array[NR-2]+array[NR-3]+array[NR-4]+array[NR-5],
      #array[NR-1]+array[NR-2]+array[NR-3]+array[NR-4]+array[NR-5],
      #array[NR]+array[NR-1]+array[NR-2]+array[NR-3]+array[NR-4]+array[NR-5],
      #"=>",
      #array[NR-5],
      #array[NR-4]"+"array[NR-5],
      #array[NR-3]"+"array[NR-4]"+"array[NR-5],
      #array[NR-2]"+"array[NR-3]"+"array[NR-4]"+"array[NR-5],
      #array[NR-1]"+"array[NR-2]"+"array[NR-3]"+"array[NR-4]"+"array[NR-5],
      #array[NR]"+"array[NR-1]"+"array[NR-2]"+"array[NR-3]"+"array[NR-4]"+"array[NR-5],
      "||",
      #array[NR]+array[NR]+array[NR]+array[NR]+array[NR],
      #array[NR-1]+array[NR-1]+array[NR-1]+array[NR-1]+array[NR-1],
      #array[NR-2]+array[NR-2]+array[NR-2]+array[NR-2]+array[NR-2],
      #array[NR-3]+array[NR-3]+array[NR-3]+array[NR-3]+array[NR-3],
      #array[NR-4]+array[NR-4]+array[NR-4]+array[NR-4]+array[NR-4],
      #array[NR-5]+array[NR-5]+array[NR-5]+array[NR-5]+array[NR-5],
      #"=>",
      #array[NR]"+"array[NR]"+"array[NR]"+"array[NR]"+"array[NR],
      #array[NR-1]"+"array[NR-1]"+"array[NR-1]"+"array[NR-1]"+"array[NR-1],
      #array[NR-2]"+"array[NR-2]"+"array[NR-2]"+"array[NR-2]"+"array[NR-2],
      #array[NR-3]"+"array[NR-3]"+"array[NR-3]"+"array[NR-3]"+"array[NR-3],
      #array[NR-4]"+"array[NR-4]"+"array[NR-4]"+"array[NR-4]"+"array[NR-4],
      #array[NR-5]"+"array[NR-5]"+"array[NR-5]"+"array[NR-5]"+"array[NR-5],
      "||",
      array[NR-1],
      array[NR-2],
      array[NR-3],
      array[NR-4],
      array[NR-5],
      "||",
      #array[NR-5],
      #array[NR-4],
      #array[NR-3],
      #array[NR-2],
      #array[NR-1]
      ""
  }'
